function getParameterByName(name)
{
  name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
  var regexS = "[\\?&]" + name + "=([^&#]*)";
  var regex = new RegExp(regexS);
  var results = regex.exec(window.location.search);
  if(results == null)
    return "";
  else
    return decodeURIComponent(results[1].replace(/\+/g, " "));
}	
		
function menucampusreset()
	{
	$('#menucampus').removeClass('menulihover');
	$('#menucampus_box').slideUp('normal', function(){
		$('#menucampus_box').removeClass('menulihoveralign_right');
		$('#menucampus_box').toggle();
		
	});
}
	
