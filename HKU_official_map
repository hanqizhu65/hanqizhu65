$.i18n.setDictionary(i18n_dict);
	
	
	initialize();
	var ondemandid = getParameterByName('id');
	var ondemandfacility_type_id = getParameterByName('facility_type_id');
	
	if ((getParameterByName('type')=='Locations') && getParameterByName('id'))
	{
		setTimeout("fetchMapPoint(" + ondemandid + ",'Locations')",3000);
		$('#navmenutab_Buildings').trigger('click');
	}
	
	if ((getParameterByName('type')=='Department') && getParameterByName('id'))
	{
		setTimeout("fetchMapPoint(" + ondemandid + ",'Department')",3000);
		$('#navmenutab_Departments').trigger('click');
	}
	
	if ((getParameterByName('type')=='facility') && getParameterByName('id'))
	{
		//togglefacilities("#aaa", 1)
		$('#navmenutab_Facilities').trigger('click');
		setTimeout("toggleOnfacilities('NavMenu-facilities-" + ondemandfacility_type_id + "', " + ondemandfacility_type_id + " )", 1000);
		//console.log( "toggleOnfacilities('NavMenu-facilities-" + ondemandfacility_type_id + "')" );
		setTimeout("fetchMapPoint(" + ondemandid + ",'facility')",3000);
		
	}
	
	
	
